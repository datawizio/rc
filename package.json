{
  "name": "@datawizio/react-components",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "typings": "./es/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./components/*": {
      "import": "./es/components/*/index.js",
      "require": "./lib/components/*/index.cjs"
    }
  },
  "files": [
    "es",
    "lib",
    "package.json",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier . --write"
  },
  "peerDependencies": {
    "antd": "^5.26.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "dependencies": {
    "antd": "^5.26.4",
    "clsx": "^2.1.1",
    "less": "^4.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "prettier": "3.6.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2"
  }
}
